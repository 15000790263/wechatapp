.categ {
  display: flex;
  padding: 10rpx 30rpx;
  background-color: #eee;
  font-size: 24rpx;
}
.categ text {
  margin-right: 30rpx;
  border: 1px solid #dde;
  background-color: #fff;
  padding: 5rpx;
}
.categ .all_show {
  border: 1px solid var(--themeColor);
  color: var(--themeColor);
}
view {
  font-size: 24rpx;
}
view navigator {
  display: flex;
  padding: 20rpx;
  height: 30vw;
  border-bottom: 1px solid #ddd;
}
view navigator .img {
  flex: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
view navigator .img image {
  width: 80%;
}
view navigator .info {
  flex: 5;
  height: 100%;
  padding-left: 20rpx;
  display: flex;
  flex-flow: column;
  justify-content: space-between;
}
view navigator .info text {
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
view navigator .info .price {
  color: var(--themeColor);
  font-size: 32rpx;
}
